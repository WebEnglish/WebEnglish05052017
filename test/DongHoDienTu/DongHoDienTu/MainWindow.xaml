<Window x:Class="DongHoDienTu.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DongHoDienTu"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <local:Timer HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100" Canvas.Left="261" Canvas.Top="106"/>
        <Button Content="Start" Name="start" FontSize="25" Width="130" Height="38" Canvas.Left="54" Canvas.Top="280" Click="btnStart"></Button>
        <Button Content="Stop" Name="stop" FontSize="25" Width="130" Height="38" Canvas.Left="235" Canvas.Top="280" Click="btnStop"/>
        <Button Content="Pause" Name="pause" FontSize="25" Width="130" Height="38" Canvas.Left="414" Canvas.Top="280" Click="btnPause"/>
        <Button Content= "Resume" Name="resume" FontSize="25" Width="130" Height="38" Canvas.Left="616" Canvas.Top="280" Click="btnResume"/>

    </Canvas>
</Window>
